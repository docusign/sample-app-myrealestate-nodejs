{
  "name": "rooms-sample-app",
  "version": "1.0.0",
  "description": "A sample application showing off the DocuSign Rooms API. This application emulates a real estate office's CRM with rooms API integration.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/docusign/sample-app-myrealestate-nodejs.git"
  },
  "keywords": [
    "Rooms",
    "API",
    "DocuSign"
  ],
  "author": "David Kennedy",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "cookies": "^0.9.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "docusign-esign": "6.5.1",
    "docusign-rooms": "2.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "faker": "^5.5.3",
    "fs": "0.0.1-security",
    "helmet": "^7.1.0",
    "memorystore": "^1.6.7",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "overrides": {
    "got": "14.2.1",
    "passport-oauth2": "1.8.0",
    "pac-proxy-agent": "7.0.1",
    "degenerator": "5.0.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  }
}
